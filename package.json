  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/sass.scss css/style.css -w",
    "compile:sass": "node-sass sass/sass.scss css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 5 versions' css/style.concat.css -o css/style.prefix.css",
    "concat:css": "concat -o css/style.concat.css css/iconfont.css css/style.comp.css",
    "compress:css": "node-sass css/style.prefixx.css css/style.compressed.css --output-style compressed"
  },
